<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 消费方应用名 -->
    <dubbo:application name="${spring.application.name}"/>

    <!-- 注册中心服务地址 -->
    <!--<dubbo:monitor protocol="registry"/>-->
    <dubbo:protocol name="dubbo" port="${dubbo.registry.port:-1}"/>
    <dubbo:registry id="zookeeper" protocol="zookeeper" address="${dubbo.registry.address}"/>

    <dubbo:consumer timeout="300000"/>

    <!--<dubbo:consumer  filter="transactionFilter" />-->
    <!-- 注册Service服务-->

    <bean id="testService" class="com.ch.cloud.kafka.service.impl.TestServiceImpl"/>
    <dubbo:service interface="com.ch.cloud.kafka.service.ITestService" ref="testService"/>

    <dubbo:service interface="com.ch.cloud.kafka.api.IClusterConfig" ref="clusterConfigImpl"/>
    <dubbo:service interface="com.ch.cloud.kafka.api.ITopicExt" ref="topicExtImpl"/>
    <bean id="searchService" class="com.ch.cloud.kafka.api.impl.SearchServiceImpl"/>
    <dubbo:service interface="com.ch.cloud.kafka.api.SearchService" ref="searchService"/>
    <!-- 引用Service服务-->

    <!--<dubbo:reference id="test2Service" interface="com.ch.cloud.test.service.ITest2Service"-->
    <!--check="false" version="1.0.0" url="" registry="zookeeper" protocol="dubbo" timeout="15000"/>-->
</beans>